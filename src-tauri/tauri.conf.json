{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "app-tauri",
  "version": "0.2.1",
  "identifier": "com.evannbl.apptauri",
  "build": {
    "frontendDist": "../out",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "Rename App",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "createUpdaterArtifacts": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "updater": {
      "endpoints": [
        "https://github.com/EvanNbl/app-tauri/releases/latest/download/latest.json"
      ],
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDZEMjk5NDJCNDQxNDQyMjMKUldRalFoUkVLNVFwYlFkMk9RK3FGRTJuRVNMNHRxNDlZTENQUUFpMm16dmFsWGJMT2FaVzF6eWQK"
    }
  }
}
